{
  "name": "tiramisu",
  "description": "pick-me-up",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images2/16.png",
      "32": "images2/32.png",
      "48": "images2/48.png",
      "128": "images2/128.png"
    }
  },
  "icons": {
    "16": "images2/16.png",
    "32": "images2/32.png",
    "48": "images2/48.png",
    "128": "images2/128.png"
  } 
}